{
    "name": "chevgenio/smspilot-notification-channel",
    "description": "SmsPilot Notifications channel for Laravel",
    "keywords": ["laravel", "notifications", "smspilot"],
    "homepage": "https://github.com/chevgenio/smspilot-notification-channel",
    "license": "MIT",
    "authors": [
        {
            "name": "Evgenii Ch",
            "email": "chevgenio@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/queue": "^8.0",
        "illuminate/notifications": "^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "mockery/mockery": "^1.3.1"
    },
    "autoload": {
        "psr-4": {
            "Chevgenio\\SmsPilot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chevgenio\\SmsPilot\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chevgenio\\SmsPilot\\SmsPilotServiceProvider"
            ]
        }
    }
}
